    <MSDeployParameterValue Include="$(DeployParameterPrefix)innosoft_easyfisConnectionString-Web.config Connection String">
      <ParameterValue>Data Source=innosoftserver2.eastasia.cloudapp.azure.com;Initial Catalog=innosoft_easyfis;User ID=sa;Password=@innosoft123</ParameterValue>
